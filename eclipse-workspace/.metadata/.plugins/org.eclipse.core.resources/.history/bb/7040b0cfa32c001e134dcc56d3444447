package projeto1;

import java.util.Scanner;

public class pp1 {
	public static void main(String[] args) {
	Scanner teclado = new Scanner(System.in);
	
	System.out.println("Diga alguns valores");
	int[] a = new int []{teclado.nextInt(), teclado.nextInt(), teclado.nextInt(), teclado.nextInt(), teclado.nextInt()};
	System.out.println(a[0], a[1], a[2], a[3], a[4]);

	}
}
