package projeto1;

import java.util.Scanner;

public class pp1 {
	public static void main(String[] args) {
	Scanner teclado = new Scanner(System.in);
	
	System.out.println("Diga alguns valores");
	int[] b = new int [5];
	b[0] =  teclado.nextInt();
	b[1] =  teclado.nextInt();
	b[2] =  teclado.nextInt();
	b[3] =  teclado.nextInt();
	b[4] =  teclado.nextInt();
	System.out.println(b[0] + " " + b[1] + " " + b[2] + " " + b[3] + " " + b[4]);

	}
}
