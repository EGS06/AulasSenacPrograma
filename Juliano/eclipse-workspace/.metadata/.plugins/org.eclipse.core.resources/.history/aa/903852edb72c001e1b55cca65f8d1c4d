package vetorMatriz;

import java.util.Arrays;

public class somarMatriz {

	public static void main(String[] args) {
	
		String[][] matriz = new String[3][3];
		
		for (int i = 0; i < matriz.length; i++) {
			for (int j = 0; j < matriz.length; j++) {
				System.out.print(matriz[i][j] +" " );
			}
			System.out.println(" ");
		}

	}

}
